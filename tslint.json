{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ]
  },
  "linterOptions": {
    "exclude": ["bin/www.ts", "node_modules"]
  },
  "rulesDirectory": []
}
